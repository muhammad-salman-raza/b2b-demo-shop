{% extends '@Gui/Layout/layout.twig' %}

{% set widget_title = 'Print' %}

{% block head_title %}
    {{ widget_title | trans }}
{% endblock %}
{% block section_title %}
    {{ widget_title | trans }}
{% endblock %}

{% block action %}
    {{ backActionButton('/printer', 'Back to List' | trans) }}
{% endblock %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' %}

        {% block widget_content %}
            <div class="row">
                <div class="col-md-6">
                    {{ form(printForm) }}
                </div>
            </div>
        {% endblock %}

    {% endembed %}
{% endblock %}
